<h1 class="full"><%= breadcrumbs :admin, :runners %></h1>
<hr class="full" />
<p class="left banner">
  <%= link_to 'New Runner', new_runner_path, :class => 'half' %>
  <%= link_to 'Import Runners', runners_import_path, :class => 'half align_right' %>
</p>
<hr class="full" />
<table class="full">
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip code</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Gender</th>
    <th>Shirt size</th>
    <th>Birthday</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @runners.each do |runner| %>
  <tr>
    <td><%= runner.first_name %></td>
    <td><%= runner.last_name %></td>
    <td><%= runner.address %></td>
    <td><%= runner.city %></td>
    <td><%= runner.state %></td>
    <td><%= runner.zip_code %></td>
    <td><%= runner.phone %></td>
    <td><%= runner.email %></td>
    <td><%= runner.gender %></td>
    <td><%= runner.shirt_size %></td>
    <td><%= runner.birthday %></td>
    <td><%= link_to 'Show', runner %></td>
    <td><%= link_to 'Edit', edit_runner_path(runner) %></td>
    <td><%= link_to 'Destroy', runner, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
<hr class="full" />
<%= link_to 'New Runner', new_runner_path, :class => 'half' %>
<%= link_to 'Import Runners', runners_import_path, :class => 'half align_right' %>