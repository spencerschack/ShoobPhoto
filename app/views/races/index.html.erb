<h1 class="full"><%= breadcrumbs :admin, :races %></h1>
<hr class="full" />
<table class="full">
  <tr>
    <th>Name</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
<% @races.each do |race| %>
  <tr>
    <td><%= race.name %></td>
    <td><%= race.date %></td>
    <td><%= link_to 'Show', race %></td>
    <td><%= link_to 'Edit', edit_race_path(race) %></td>
    <td><%= link_to 'Destroy', race, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
<hr class="full" />
<%= link_to 'New Race', new_race_path, :class => 'full' %>
